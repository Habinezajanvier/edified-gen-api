{
  "name": "teahouse",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "dev:start": "nodemon --exec babel-node server.js",
    "start": "node ./dist/index.js",
    "lint": "./node_modules/.bin/eslint src/",
    "build": "rm -rf dist && babel src --out-dir ./dist --source-maps",
    "migration:undo": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.1.1",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.2",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/register": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-airbnb": "^4.1.0",
    "debug": "~2.6.9",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4"
  }
}
