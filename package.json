{
  "name": "teahouse",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12.13.1"
  },
  "scripts": {
    "dev:start": "nodemon --exec babel-node src/server.js",
    "start": "npm run migrate && npm run seed && node ./dist/server.js",
    "lint": "./node_modules/.bin/eslint src/",
    "build": "rm -rf dist && babel src --out-dir ./dist --source-maps",
    "migration:undo": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.4.2",
    "aws-sdk": "^2.869.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.12",
    "@babel/register": "^7.13.8",
    "babel-eslint": "^10.0.3",
    "babel-preset-airbnb": "^5.0.0",
    "debug": "~4.3.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7"
  }
}
